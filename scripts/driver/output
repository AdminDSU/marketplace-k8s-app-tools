        volumeMounts:
        - mountPath: /var/run/secret/kasten.io/k10-api-key
          name: k10-api-key
          readOnly: true
      - args:
        - tail
        - -f
        - /dev/null
        image: gcr.io/kasten-images/service-proxy:hash-bc17b40
