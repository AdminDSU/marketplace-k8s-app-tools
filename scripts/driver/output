          valueFrom:
            configMapKeyRef:
              key: clustername
              name: k10-config
        - name: MODEL_STORE_DIR
          valueFrom:
            configMapKeyRef:
              key: modelstoredirname
              name: k10-config
+ kubectl apply --namespace=apptest-8f0887bb --filename=/data/resources.yaml
        - name: AWS_FIREHOSE_DELIVERY_STREAM_NAME
          valueFrom:
            configMapKeyRef:
              key: firehosedeliverystreamname
              name: k10-config
        - name: LOG_LEVEL
          valueFrom:
            configMapKeyRef:
              key: loglevel
              name: k10-config
        - name: POD_NAMESPACE
          valueFrom:
            fieldRef:
              fieldPath: metadata.namespace
        image: gcr.io/kasten-images/auth:1.1.13
        imagePullPolicy: Always
